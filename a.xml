<?xml version="1.0" encoding="UTF-8"?>
<wfs:WFS_Capabilities xmlns:wfs="http://www.opengis.net/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/wfs http://schemas.opengis.net/wfs/1.1.0/wfs.xsd" version="1.1.0">
    <wfs:ServiceIdentification>
        <wfs:Title>Sample WFS Service</wfs:Title>
        <wfs:Abstract>A sample WFS service for demo purposes.</wfs:Abstract>
        <wfs:Keywords>
            <wfs:Keyword>WFS</wfs:Keyword>
            <wfs:Keyword>Geospatial</wfs:Keyword>
        </wfs:Keywords>
        <wfs:ServiceType>OGC:WFS</wfs:ServiceType>
        <wfs:ServiceTypeVersion>1.1.0</wfs:ServiceTypeVersion>
    </wfs:ServiceIdentification>

    <wfs:ServiceProvider>
        <wfs:ProviderName>Sample Service Provider</wfs:ProviderName>
        <wfs:ProviderSite xlink:href="http://www.sampleprovider.com"/>
    </wfs:ServiceProvider>

    <wfs:OperationsMetadata>
        <wfs:Operation name="GetCapabilities">
            <wfs:DCP>
                <wfs:HTTP>
                    <wfs:Get xlink:href="http://www.sampleprovider.com/wfs?service=WFS&version=1.1.0&request=GetCapabilities"/>
                </wfs:HTTP>
            </wfs:DCP>
        </wfs:Operation>

        <wfs:Operation name="DescribeFeatureType">
            <wfs:DCP>
                <wfs:HTTP>
                    <wfs:Get xlink:href="http://www.sampleprovider.com/wfs?service=WFS&version=1.1.0&request=DescribeFeatureType"/>
                </wfs:HTTP>
            </wfs:DCP>
        </wfs:Operation>

        <wfs:Operation name="GetFeature">
            <wfs:DCP>
                <wfs:HTTP>
                    <wfs:Get xlink:href="http://www.sampleprovider.com/wfs?service=WFS&version=1.1.0&request=GetFeature"/>
                </wfs:HTTP>
            </wfs:DCP>
        </wfs:Operation>
    </wfs:OperationsMetadata>

    <wfs:FeatureTypeList>
        <wfs:FeatureType>
            <wfs:Name>Building</wfs:Name>
            <wfs:Title>Buildings in the City</wfs:Title>
            <wfs:Abstract>Geospatial information about buildings in the city.</wfs:Abstract>
            <wfs:Keywords>
                <wfs:Keyword>Building</wfs:Keyword>
                <wfs:Keyword>City</wfs:Keyword>
            </wfs:Keywords>
            <wfs:DefaultCRS>EPSG:4326</wfs:DefaultCRS>
            <wfs:BoundingBox>
                <ows:LowerCorner>0.0 0.0</ows:LowerCorner>
                <ows:UpperCorner>10.0 10.0</ows:UpperCorner>
            </wfs:BoundingBox>
        </wfs:FeatureType>
        
        <wfs:FeatureType>
            <wfs:Name>Road</wfs:Name>
            <wfs:Title>Roads in the City</wfs:Title>
            <wfs:Abstract>Geospatial information about roads in the city.</wfs:Abstract>
            <wfs:Keywords>
                <wfs:Keyword>Road</wfs:Keyword>
                <wfs:Keyword>City</wfs:Keyword>
            </wfs:Keywords>
            <wfs:DefaultCRS>EPSG:4326</wfs:DefaultCRS>
            <wfs:BoundingBox>
                <ows:LowerCorner>0.0 0.0</ows:LowerCorner>
                <ows:UpperCorner>10.0 10.0</ows:UpperCorner>
            </wfs:BoundingBox>
        </wfs:FeatureType>
    </wfs:FeatureTypeList>
</wfs:WFS_Capabilities>
